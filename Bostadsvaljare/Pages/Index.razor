@page "/"

@using System.Net.Http
@using Syncfusion.Blazor.Grids
@using Syncfusion.Blazor.SplitButtons

@inject HttpClient Http
@inject IJSRuntime JSRuntime
@inject NavigationManager navManager 


<div id="selector">
    <img id="overview" src="/IMG/Imagemapbild.jpg" usemap="#housingmap">
</div>
<map name="housingmap" id="map">
    <area status="available" href="#" @onmouseover="@(e => ShowTooltip(e, "A table!"))" @onmouseout="HideTooltip" @onclick="@(() => HousingClicked(""))" shape="poly" coords="1468,899,1479,825,1154,813,1127,854,1270,899,1369,900">
    @*<area shape="rect" coords="34,44,270,350" alt="Computer" href="https://www.newegg.com/" @onmouseover="@(e => ShowTooltip(e, "This is my computer"))" @onmouseout="HideTooltip" id="rm1" @onclick="@(e => SelectColorChange(e, "rm1"))">
    <area shape="rect" coords="290,172,333,250" alt="Phone" href="https://www.samsung.com/us/mobile/phones/" @onmouseover="@(e => ShowTooltip(e, "This is my phone."))" @onmouseout="HideTooltip" id="rm2" @onclick="@(e => SelectColorChange(e, "rm2"))">
    <area shape="circle" coords="337,300,44" alt="Cup of coffee" href="https://colorlib.com/wp/coffee-shop-websites/" @onmouseover="@(e => ShowTooltip(e, "This is my cup of the coffee."))" @onmouseout="HideTooltip" id="rm3" @onclick="@(e => SelectColorChange(e, "rm3"))">*@
</map>

<div id="tipmsg"></div>

<BSImage src="IMG/imagemapbild.jpg"
         hidden="@(!HousingSelected)"
         Style="
             width: 50%;
             margin-bottom: 10px;
             margin-right: 5px;
             display: inline-block;
         ">
</BSImage>

<div id="housing_list" hidden="@(!HousingSelected)">
    <SfGrid DataSource="DataSource" AllowSorting="true" AllowFiltering="true" AllowPaging="true" EnableHover="true">
        <GridPageSettings PageSize="18" />
        <GridFilterSettings Type="Syncfusion.Blazor.Grids.FilterType.FilterBar" Mode="FilterBarMode.Immediate" />
        <GridColumns>
            <GridColumn Field="@nameof(Houses.HouseNumber)" HeaderText="Husnummer" />
            <GridColumn Field="@nameof(Houses.Sqm)" HeaderText="Storlek" />
            <GridColumn Field="@nameof(Houses.Price)" Format="C2" HeaderText="Pris" />
            <GridColumn Field="@nameof(Houses.LandArea)" HeaderText="Tomtstorlek" />
            <GridColumn Field="@nameof(Houses.Status)" HeaderText="Status" customAttributes=@(new { @class = "e-attr" })>
                <Template>
                    @{
                        var House = (context as Houses);
                        <div class="image">
                            @if (House.Status == "Såld")
                            {
                                <img src="https://upload.wikimedia.org/wikipedia/commons/archive/c/c3/20090215162203%21Location_dot_dark_red.svg"
                                     height="25"
                                     width="25" />
                            }
                            @if (House.Status == "Reserverad")
                            {
                                <img src=" https://upload.wikimedia.org/wikipedia/commons/9/91/Location_dot_orange.svg"
                                     height="25"
                                     width="25" />
                            }
                            @if (House.Status == "Ledig")
                            {
                                <img src=" https://upload.wikimedia.org/wikipedia/commons/0/0e/Ski_trail_rating_symbol-green_circle.svg"
                                     height="25"
                                     width="25" />
                            }
                            <span id="Emptext">@House.Status</span>
                        </div>
                    }
                </Template>
            </GridColumn>
        </GridColumns>
    </SfGrid>
</div>

<div id="sun_study">
    <SfDropDownButton IconCss="e-ddb-icons e-profile" CssClass="e-caret-hide e-background">
        <DropDownButtonEvents OnOpen="OpenTop" ItemSelected="ChangeSunStudy" />
        <DropDownButtonItems>
            <DropDownButtonItem Id="0" Text="Morgon" IconCss="e-ddb-icons"></DropDownButtonItem>
            <DropDownButtonItem Id="1" Text="Mitt på dagen" IconCss="e-ddb-icons"></DropDownButtonItem>
            <DropDownButtonItem Id="2" Text="Kväll" IconCss="e-ddb-icons"></DropDownButtonItem>
        </DropDownButtonItems>
    </SfDropDownButton>
</div>


